











#include <iostream>
using namespace std;

   int sum(int a,int b){
      return a+b; 
   }
   
   
   
   int main() {
       int n1 ,n2, add;
       cout << "Enter the no 1:" ;
       cin >> n1;
       cout << "Enter the no 2:" ;
       cin >> n2;
       add=sum(n1,n2);
       cout << "Sum of two numbers is :"  << add;

    return 0;
}
